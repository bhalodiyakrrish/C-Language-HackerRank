#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    int sum = 0;
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i = 0;i < n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i = 0;i < n;i++)
    {
        sum += arr[i];
    }
    printf("%d",sum);
      
    return 0;
}

# Input (stdin):
6
16 13 7 2 1 12 
# Expected Output:
51
