#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n;
    scanf("%d", &n);
    int mod;
    int sum = 0;
    while (n > 0){
        mod = n%10;
        sum+= mod;
        n = n/10; 
    }
    printf("%d",sum);
    
    return 0;
}

# Input (stdin):
10564
# Expected Output:
16
